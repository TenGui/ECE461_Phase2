{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "outDir": "./",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["./*.ts", "./endpoints/*.ts", "./static/*.ts"],
    "exclude": ["node_modules", "src/*.ts"]
  }